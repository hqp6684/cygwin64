if [ ! -f /etc/php5/php.ini ]
then
    /usr/bin/mkdir -p /etc/php5
    /usr/bin/cp /etc/defaults/etc/php5/php.ini /etc/php5/php.ini
fi

if [ ! -f /etc/php5/php-fpm.conf ]
then
    /usr/bin/mkdir -p /etc/php5
    /usr/bin/cp /etc/defaults/etc/php5/php-fpm.conf /etc/php5/php-fpm.conf
fi

if [ ! -f /etc/httpd/conf.d/php5.conf ]
then
    /usr/bin/mkdir -p /etc/httpd/conf.d
    /usr/bin/cp /etc/defaults/etc/httpd/conf.d/php5.conf /etc/httpd/conf.d/php5.conf
fi

